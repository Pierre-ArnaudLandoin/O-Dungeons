<form method="post" action="{{ path('app_admin_background_delete', {'id': background.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer cet historique ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ background.id) }}">
    <button class="btn btn-danger mb-2">Supprimer</button>
</form>
